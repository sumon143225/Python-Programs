class Solution:
		def gcdOfStrings(self, str1: str, str2: str) -> str:
			# Check if they have a common divisor pattern
			if str1 + str2 != str2 + str1:
				return ""
			
			# Find GCD of lengths
			from math import gcd
			length = gcd(len(str1), len(str2))
			
			# Return the common prefix of that length
			return str1[:length]
			
